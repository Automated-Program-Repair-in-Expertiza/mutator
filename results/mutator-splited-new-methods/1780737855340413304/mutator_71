class QuestionnairesController < ApplicationController
  def update_quiz
    @questionnaire = Questionnaire.find(params[:id])
    if @questionnaire.nil?
      redirect_to(controller: "submitted_content", action: "view", id: params[:pid])
    end
    nil
    redirect_to(controller: "submitted_content", action: "view", id: params[:pid])
  end
end
