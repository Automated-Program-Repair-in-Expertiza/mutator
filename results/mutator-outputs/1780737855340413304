+          if (params[nil][@question.id.to_s][@question.type][1.to_s][:iscorrect] == "True")
             if (quiz_question_choice.txt == "True")
               quiz_question_choice.update_attributes(iscorrect: "1")
             else
               quiz_question_choice.update_attributes(iscorrect: "0")
             end
